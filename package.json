{
  "name": "keyv-nest",
  "version": "0.0.4",
  "description": "Nest multi-level keyv caches, boost user-experiences",
  "keywords": [],
  "homepage": "https://github.com/snomiao/keyv-nest#readme",
  "bugs": {
    "url": "https://github.com/snomiao/keyv-nest/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/snomiao/keyv-nest.git"
  },
  "license": "MIT",
  "author": "snomiao <snomiao@gmail.com>",
  "exports": {
    "import": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "main": "./dist/index.js",
  "module": "index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "*.ts",
    "dist"
  ],
  "scripts": {
    "build": "bun build:type && bun build:js && bun build:umd",
    "build:js": "bun build . --outdir=dist",
    "build:type": "bunx --bun tsc -d --noEmit false --emitDeclarationOnly --outDir dist",
    "build:umd": "bunx rollup dist/index.js --file dist/index.umd.js --format umd --name {}",
    "prerelease": "bun run build && bun run test",
    "release": "bunx standard-version && git push --follow-tags && npm publish",
    "test": "bun test"
  },
  "dependencies": {
    "@keyv/sqlite": "^3.6.7",
    "ansi-styles": "^5.2.0",
    "braces": "^3.0.3",
    "bun-types": "^1.1.17",
    "chalk": "^4.1.2",
    "ci-info": "^3.9.0",
    "color-convert": "^2.0.1",
    "color-name": "^1.1.4",
    "diff-sequences": "^29.6.3",
    "escape-string-regexp": "^2.0.0",
    "expect": "^29.7.0",
    "fill-range": "^7.1.1",
    "graceful-fs": "^4.2.11",
    "has-flag": "^4.0.0",
    "is-number": "^7.0.0",
    "jest-diff": "^29.7.0",
    "jest-get-type": "^29.6.3",
    "jest-matcher-utils": "^29.7.0",
    "jest-message-util": "^29.7.0",
    "jest-util": "^29.7.0",
    "js-tokens": "^4.0.0",
    "micromatch": "^4.0.7",
    "phpdie": "^1.2.12",
    "picocolors": "^1.0.1",
    "picomatch": "^2.3.1",
    "pretty-format": "^29.7.0",
    "react-is": "^18.3.1",
    "slash": "^3.0.0",
    "stack-utils": "^2.0.6",
    "supports-color": "^7.2.0",
    "to-regex-range": "^5.0.1",
    "undici-types": "^5.26.5"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/jest": "^29.5.12",
    "keyv": "^4.5.4",
    "tiny-lru": "^11.2.11",
    "typescript": "^5.5.4"
  },
  "type": "module"
}
